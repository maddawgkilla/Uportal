<% include partial/header %>

<%
if(success.length > 0) {
%>
    <div class="alert alert-success"><%=success%></div>
<%
}
%>

<table class="table table-hover">
    <thead>
        <tr>
            <th>Player ID</th>
            <th>Year ID</th>
        </tr>
    </thead>
    <tbody>
        <% paidOrders.forEach(item => { %>
            <tr>
                <td>
                    <%=item.playerID%>
                </td>
                <td>
                    <%=item.yearID%>
                </td>
            </tr>
            <% }); %>
    </tbody>
</table>

<% include partial/footer %>
